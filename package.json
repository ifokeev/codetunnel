{
  "name": "codetunnel-monorepo",
  "version": "0.1.0",
  "private": true,
  "description": "CodeTunnel - Instant, secure development tunnels for web terminals",
  "scripts": {
    "dev": "pnpm --filter @codetunnel/desktop dev",
